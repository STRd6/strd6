= error_messages_for :user

- form_for :user, :url => users_path do |f|

  = p_label 'login', f.text_field(:login)

  = p_label 'email', f.text_field(:email)

  = p_label 'password', f.password_field(:password)

  = p_label 'password_confirmation', f.password_field(:password_confirmation)

  %p= submit_tag 'Sign up'